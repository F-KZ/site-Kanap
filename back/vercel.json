{
    "version": 2,
    "builds": [
        {
            "src" : "./index.js",
            "use":"@vercel/node"
        },
        {
            "src": "front/**",
            "use": "@vercel/static"
          }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"../front/html/index.html"
        }
    ]
}